<h2>Register</h2>
<form method="POST" action="/register">
  <label for='fname'>First Name </label>
  <input type='text' name='[user]fname' id='fname'/>
  <br/>
  <label for='lname'>Last Name </label>
  <input type='text' name='[user]lname' id='name'/>
  <br/>
  <label for='email'>Email </label>
  <input type='text' name='[user]email' id='email'/>
  <br/>
  <label for='bio'>Bio: </label>
  <input type='text' name='[profile]bio' id='bio'/>
  <br>
  <label for='location'>location: </label>
  <input type='text' name='[profile]location' id='location'/>
  <br>
  <label for='password'>Password</label>
  <input type='password' name='[user]password' id='password'/>
   <label for='confirm_password'>Confirm Password </label>
   <input type='password' name='confirm_password' id='confirm_password'/>
  
  <br />
  <input type='submit' />
</form>
